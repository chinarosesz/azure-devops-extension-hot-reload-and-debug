{
  "id": "sample-dev",
  "public": false,
  "manifestVersion": 1,
  "version": "1.0.0",
  "name": "sample-dev",
  "description": "Sample Azure DevOps extension using hot reload",
  "publisher": "litra",
  "demands": [ "api-version/3.0" ],
  "categories": [ "Azure Repos" ],
  "baseUri": "https://localhost:4000",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "icons": {
    "default": "img/world.png"
  },
  "files": [
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "sample-dev-hub-group",
      "type": "ms.vss-web.hub-group",
      "description": "Sample hub group",
      "targets": [ "ms.vss-web.project-hub-groups-collection" ],
      "properties": {
        "name": "Sample-Dev",
        "icon": {
          "dark": "img/world.png",
          "light": "img/world.png"
        }
      }
    },
    {
      "id": "default-hub",
      "type": "ms.vss-web.hub",
      "targets": [ ".sample-dev-hub-group" ],
      "properties": {
        "uri": "dist/main.html"
      }
    }
  ]
}
